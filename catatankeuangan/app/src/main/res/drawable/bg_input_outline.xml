<?xml version="1.0" encoding="utf-8"?>
<!-- Latar untuk "Outlined Box" (misal: Jumlah, Keterangan) -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="@color/white" /> <!-- Latar belakang putih -->
    <stroke
        android:width="1dp"
        android:color="@color/biru_primer" /> <!-- Garis tepi biru -->
    <corners android:radius="30dp" /> <!-- Sudut membulat -->
</shape>